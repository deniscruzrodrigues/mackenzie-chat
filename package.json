{
  "name": "mackenzie-chat",
  "displayName": "Mackenzie Chat",
  "description": "Analisador de código",
  "version": "0.0.1",
  "publisher": "decruro",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:mackenzieChat",
    "onCommand:codeAnalyzer.openChat"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeAnalyzer.openChat",
        "title": "Analisador de Código: Abrir Chat"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-analyzer",
          "icon": "resources/icon.svg",
          "title": "Code Analyzer"
        }
      ]
    },
    "views": {
      "code-analyzer": [
        {
          "id": "mackenzieChat",
          "name": "Chat",
          "icon": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "debug": "node --inspect-brk=9229 ./node_modules/webpack/bin/webpack.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deniscruzrodrigues/mackenzie-chat"
  },
  "license": "MIT"
}
